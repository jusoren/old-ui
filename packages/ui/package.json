{
  "name": "ui",
  "version": "0.0.10",
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./base.css": {
      "import": "./dist/base.css",
      "require": "./dist/base.css"
    },
    "./tailwind.config.js": {
      "import": "./dist/tailwind.config.js",
      "require": "./dist/tailwind.config.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jusoren/ui"
  },
  "homepage": "https://ui.jusoren.com",
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "dev": "tsup --watch",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
